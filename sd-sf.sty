\def\@symmet[#1,#2]#3#4#5{%
    \@ifnextchar*%
    {\@@symmet[#1,#2]{#3}{#4}{#5}}%
    {\@@symmet[#1,#2]{#3}{#4}{#5}*{}}%
}

\def\@@symmet[#1,#2]#3#4#5*#6{%
    \def\@sw{#6}\def\@sww{}%
    \def\@sw{#4}\def\@swa{1}\def\@swb{2}\def\@swc{3}\def\@swd{4}\def\@swe{.}%
    \ifx\@sw\@swe\def\@c{}%
    \else\ifx\@sw\@swa\def\@c{3}%
    \else\ifx\@sw\@swb\def\@c{4}%
    \else\ifx\@sw\@swc\def\@c{1}%
    \else\ifx\@sw\@swd\def\@c{2}%
    \else\def\@c{\@@@symmet#4}%
    \fi\fi\fi\fi\fi%
    \def\@sw{#5}\def\@swe{.}\def\@swf{n}\def\@swg{s}\def\@swh{e}\def\@swi{w}%
    \def\@swj{v}\def\@swk{h}\def\@swl{r}\def\@swm{l}\def\@swn{+}\def\@swo{x}%
    \ifx\@sw\@swe\def\@d{.}%
    \else\ifx\@sw\@swf\def\@d{s}%
    \else\ifx\@sw\@swg\def\@d{n}%
    \else\ifx\@sw\@swh\def\@d{w}%
    \else\ifx\@sw\@swi\def\@d{e}%
    \else\ifx\@sw\@swa\def\@d{3}%
    \else\ifx\@sw\@swb\def\@d{4}%
    \else\ifx\@sw\@swc\def\@d{1}%
    \else\ifx\@sw\@swd\def\@d{2}%
    \else\ifx\@sw\@swj\def\@d{v}%
    \else\ifx\@sw\@swk\def\@d{h}%
    \else\ifx\@sw\@swl\def\@d{r}%
    \else\ifx\@sw\@swm\def\@d{l}%
    \else\ifx\@sw\@swn\def\@d{+}%
    \else\ifx\@sw\@swo\def\@d{x}%
    \else\@symdir#5]\def\@d{\@symD}%
    \fi\fi\fi\fi\fi\fi\fi\fi\fi\fi\fi\fi\fi\fi\fi%
    \def\@sw{#6}\def\@swa{}\def\@swb{\@symD}%
    \ifx\@sw\@swa
        \ifx\@d\@swb\@dancer[-#1,-#2]{#3}{\@c}{0\@d}%
        \else\@dancer[-#1,-#2]{#3}{\@c}{\@d}%
        \fi%
    \else
        \ifx\@d\@swb\@dancer[-#1,-#2]{#3}{\@c}{0\@d}*{\@@@symmet#6}%
        \else\@dancer[-#1,-#2]{#3}{\@c}{\@d}*{\@@@symmet#6}%
        \fi%
    \fi%
}

\def\@symdir{\@ifnextchar0{\@@symdir}{\@@symdir0}}
\def\@@symdir0#1]{\global\@symD=#1\global\advance\@symD 180}

\def\@@@symmet#1{%
    \ifx#1\danc\def\@symmet@{\symdanc}%
    \else\ifx#1\boy\def\@symmet@{\symboy}%
    \else\ifx#1\girl\def\@symmet@{\symgirl}%
