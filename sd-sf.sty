                 &          & \@dancer[0,-25]#5 &        &                  \cr
                 &          & \@dancer[0,-25]#4 &        &                  \cr
\@dancer[25,0]#2 & \@dancer[25,0]#3 & & \@symmet[25,0]#3 & \@symmet[25,0]#2 \cr
                 &          & \@symmet[0,-25]#4 &        &                  \cr
