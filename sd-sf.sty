    \@dancer[26,-26]#2  &        &        & \@dancer[-26,-26]#5 \cr
              & \@dancer[-3,3]#3 & \@dancer[3,3]#4  &           \cr
              & \@symmet[3,3]#4  & \@symmet[-3,3]#3 &           \cr
