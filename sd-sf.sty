    \else\ifx#1\spot\def\@symmet@{\symspot}%
    \else\ifx#1\sdarrow\def\@symmet@{\symarrow}%
    \else\def\@symmet@{#1}%
    \fi\fi\fi\fi\fi\fi%
    \@symmet@%
}

