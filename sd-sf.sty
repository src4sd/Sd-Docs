\def\@@shvthar[#1][#2,#3,#4,#5]{%
    \sdform[#1]
    {
                 & \@dancer[50,0]#5 &                  &                  \cr
                 & \@dancer[50,0]#4 & \@symmet[0,50]#3 & \@symmet[0,50]#2 \cr
\@dancer[0,50]#2 & \@dancer[0,50]#3 & \@symmet[50,0]#4 &                  \cr
                 &                  & \@symmet[50,0]#5 &                  \cr
    }%
}

\def\slrthar{\@ifnextchar[{\@slrthar}{\@slrthar[1,1]}}
\def\@slrthar[#1]#2{\@@slrthar[#1][#2]}
\def\@@slrthar[#1][#2,#3,#4,#5]{{%
    \e@border{-0.42}\w@border{-0.42}\n@border{-0.42}\s@border{-0.42}%
    \sdform[#1]
    {
    \@dancer[42,-42]#2  &          &          & \@dancer[-42,-42]#5 \cr
             & \@dancer[14,-14]#3  & \@dancer[-14,-14]#4 &          \cr
             & \@symmet[-14,-14]#4 & \@symmet[14,-14]#3  &          \cr
    \@symmet[-42,-42]#5 &          &          & \@symmet[42,-42]#2  \cr
    }%
}}

