\def\@shcircle[#1]#2{\@@shcircle[#1][#2]}
\def\@@shcircle[#1][#2,#3,#4,#5]{%
    \sdform[#1]
    {
                 &         \@dancer[50,0]#3  &       &                      \cr
\@symmet[0,50]#5 & \@dancer[-56,56]#2 & \@dancer[56,56]#4 &                 \cr
                 & \@symmet[56,56]#4 & \@symmet[-56,56]#2 & \@dancer[0,50]#5\cr
                 &         &  \@symmet[50,0]#3       &                      \cr
    }%
}

\def\sscircle{\@ifnextchar[{\@sscircle}{\@sscircle[1,1]}}
\def\@sscircle[#1]#2{\@@sscircle[#1][#2]}
\def\@@sscircle[#1][#2,#3,#4,#5]{{%
    \e@border{-0.11}\w@border{-0.11}\n@border{-0.11}\s@border{-0.11}%
    \sdform[#1]
    {
               & \@dancer[-7,-11]#3 & \@dancer[7,-11]#4  &               \cr
\@dancer[11,7]#2  &                 &                & \@dancer[-11,7]#5 \cr
\@symmet[-11,7]#5 &                 &                & \@symmet[11,7]#2  \cr
               & \@symmet[7,-11]#4  & \@symmet[-7,-11]#3 &               \cr
    }%
}}

